{
  "extends": [
    "tslint-eslint-rules"
  ],
  "rules": {
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "indent": [
      true,
      "spaces"
    ],
    "import-blacklist": [
      true,
      "d3"
    ],
    "import-spacing": true,
    "object-curly-spacing": [
      true,
      "always"
    ],
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-debugger": true,
    "no-console": [
      true,
      "log"
    ],
    "no-internal-module": true,
    "no-trailing-whitespace": [
      true,
      "ignore-comments"
    ],
    "no-unsafe-finally": true,
    "no-var-keyword": true,
    "no-default-export": false,
    "no-unused-variable": [
      true,
      "react"
    ],
    "no-reference": true,
    "one-line": [
      true,
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "double"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "prefer-const": true
  },
  "linterOptions": {
    "exclude": [
      "*.snap"
    ]
  }
}